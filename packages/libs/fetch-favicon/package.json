{
  "name": "@pkerschbaum-homepage/fetch-favicon",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "postinstall": "ts-patch install -s",
    "nuke": "pnpm run nuke:build",
    "nuke:build": "rimraf dist *.tsbuildinfo",
    "build": "tsc -p ./tsconfig.project.json",
    "lib:watch": "pnpm run build --watch --preserveWatchOutput",
    "lint": "eslint --max-warnings 0",
    "lint:package": "pnpm run lint .",
    "lint:package:fix": "pnpm run lint:package --fix",
    "compile": "tsc -p ./tsconfig.project.json --noEmit"
  },
  "dependencies": {
    "@pkerschbaum-homepage/commons": "workspace:1.0.0",
    "@pkerschbaum-homepage/commons-node": "workspace:1.0.0",
    "@pkerschbaum-homepage/mdx": "workspace:1.0.0",
    "@pkerschbaum-homepage/shared-node": "workspace:1.0.0",
    "@pkerschbaum/ts-utils": "^0.0.1",
    "@types/mdast": "^3.0.10",
    "esbuild": "^0.14.48",
    "mdx-bundler": "^9.0.1",
    "puppeteer": "^15.3.2",
    "tiny-invariant": "^1.2.0",
    "unist-util-visit": "^4.1.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.25",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-code-import-patterns": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-regexp": "^1.7.0",
    "prettier": "^2.7.1",
    "ts-patch": "^2.0.1",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
