{
  "affected": {
    "defaultBase": "master"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "compile", "lint:package", "lint:package:fix"]
      }
    }
  },
  "namedInputs": {
    "sources": ["{projectRoot}/src/**/*"],
    "package-files": ["{projectRoot}/package.json"],
    "tsc-files": ["{projectRoot}/tsconfig.project.json", "{projectRoot}/*.tsbuildinfo"],
    "outputs": ["{projectRoot}/dist/**/*"]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "sources",
        "package-files",
        "tsc-files",
        { "input": "outputs", "projects": "dependencies" }
      ],
      "outputs": ["{projectRoot}/dist/**/*"],
      "dependsOn": ["^build"]
    },
    "compile": {
      "inputs": [
        "sources",
        "package-files",
        "tsc-files",
        { "input": "outputs", "projects": "dependencies" }
      ],
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "lint:package": {
      "inputs": [
        "sources",
        "package-files",
        "tsc-files",
        { "input": "outputs", "projects": "dependencies" }
      ],
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "lint:package:fix": {
      "inputs": [
        "sources",
        "package-files",
        "tsc-files",
        { "input": "outputs", "projects": "dependencies" }
      ],
      "outputs": ["{projectRoot}/src/**/*"],
      "dependsOn": ["^build"]
    }
  }
}
